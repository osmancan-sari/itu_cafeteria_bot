<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <xacro:include filename="inertial_macros.xacro"/>

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="gray">
    <color rgba="0.65 0.65 0.65 1"/>
  </material>
  <material name="dark gray">
  <color rgba="0.3 0.3 0.3 1"/>
</material>

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </collision>
    <xacro:inertial_cylinder mass="1" length="0.6" radius="0.2">
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>
   <gazebo reference="base_link">
        <material>Gazebo/White</material>
    </gazebo>


  <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint"></link>




  <link name="right_leg">
    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.1 0.2"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
    </collision>
    <xacro:inertial_box mass="0.5" x="0.6" y="0.1" z="0.2">
        <origin xyz="0 0 -0.3" rpy="0 1.57075 0"/>
    </xacro:inertial_box>
  </link>
  <gazebo reference="right_leg">
      <material>Gazebo/White</material>
  </gazebo>

  <joint name="base_to_right_leg" type="fixed">
    <parent link="base_link"/>
    <child link="right_leg"/>
    <origin xyz="0 -0.22 0.25"/>
  </joint>

  <link name="right_base">
    <visual>
      <geometry>
        <box size="0.4 0.1 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.4 0.1 0.1"/>
      </geometry>
    </collision>
    <xacro:inertial_box mass="0.5" x="0.4" y="0.1" z="0.1">
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>
  <gazebo reference="right_base">
      <material>Gazebo/White</material>
  </gazebo>

  <joint name="right_base_joint" type="fixed">
    <parent link="right_leg"/>
    <child link="right_base"/>
    <origin xyz="0 0 -0.6"/>
  </joint>

  
  <link name="right_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.5" length="0.05" radius="0.08">
        <origin xyz="0 0 0" rpy="1.57075 0 0"/>
    </xacro:inertial_cylinder>
  </link>
  <gazebo reference="right_back_wheel">
      <material>Gazebo/Black</material>
  </gazebo>

  <joint name="right_back_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="right_base"/>
    <child link="right_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.133333333333 -0.075 0"/>
  </joint>

  <link name="left_leg">
    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.6 0.1 0.2"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
    </collision>

    <xacro:inertial_box mass="0.5" x="0.6" y="0.1" z="0.2">
        <origin xyz="0 0 -0.3" rpy="0 1.57075 0"/>
    </xacro:inertial_box>
  </link>
  <gazebo reference="left_leg">
      <material>Gazebo/White</material>
  </gazebo>

  <joint name="base_to_left_leg" type="fixed">
    <parent link="base_link"/>
    <child link="left_leg"/>
    <origin xyz="0 0.22 0.25"/>
  </joint>

  <link name="left_base">
    <visual>
      <geometry>
        <box size="0.4 0.1 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.4 0.1 0.1"/>
      </geometry>
    </collision>
     <xacro:inertial_box mass="0.5" x="0.4" y="0.1" z="0.1">
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>
  <gazebo reference="left_base">
      <material>Gazebo/White</material>
  </gazebo>

  <joint name="left_base_joint" type="fixed">
    <parent link="left_leg"/>
    <child link="left_base"/>
    <origin xyz="0 0 -0.6"/>
  </joint>


  <link name="left_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.5" length="0.05" radius="0.08">
        <origin xyz="0 0 0" rpy="1.57075 0 0"/>
    </xacro:inertial_cylinder>
  </link>
  <gazebo reference="left_back_wheel">
      <material>Gazebo/Black</material>
  </gazebo>

  <joint name="left_back_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="left_base"/>
    <child link="left_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.133333333333 0.075 0"/>
  </joint>


    <joint name="caster_wheel_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_wheel"/>
        <origin xyz="0.133333333333 0 -0.35"/>
    </joint>


    <link name="caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.08"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.08"/>
            </geometry>
        </collision>
        <xacro:inertial_sphere mass="0.1" radius="0.08">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>

    <gazebo reference="caster_wheel">
        <material>Gazebo/Black</material>
        <mu1 value="0.001"/>
        <mu2 value="0.001"/>
    </gazebo>

  <link name="head">
    <visual>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
    </collision>

    <xacro:inertial_sphere mass="0.5" radius="0.2">
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_sphere>
  </link>
  <gazebo reference="head">
      <material>Gazebo/White</material>
  </gazebo>

  <joint name="head_swivel" type="fixed">
    <parent link="base_link"/>
    <child link="head"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.3"/>
  </joint>



<link name="tray_pole">
  <visual>
    <origin xyz="0.06 0 0" rpy="0 1.5708 0"/>
    <geometry>
      <cylinder length="0.12" radius="0.01"/>
    </geometry>
    <material name="dark gray"/>
  </visual>

  <collision>
    <origin xyz="0.06 0 0" rpy="0 1.5708 0"/>
    <geometry>
      <cylinder length="0.12" radius="0.01"/>
    </geometry>
  </collision>

  <xacro:inertial_cylinder mass="0.1" length="0.12" radius="0.01">
        <origin xyz="0.06 0 0" rpy="0 1.5708 0"/>
    </xacro:inertial_cylinder>
</link>
<gazebo reference="tray_pole">
      <material>Gazebo/DarkGrey</material>
  </gazebo>

<joint name="base_to_pole" type="fixed">
  <parent link="base_link"/>
  <child link="tray_pole"/>
  <origin xyz="0.18 0 0.20" rpy="0 0 0"/>
</joint>

<link name="tray">
  <visual>
    <geometry>
      <box size="0.30 0.40 0.02"/>
    </geometry>
    <material name="dark gray"/>
  </visual>

  <collision>
    <geometry>
      <box size="0.30 0.40 0.02"/>
    </geometry>
  </collision>

  <xacro:inertial_box mass="0.05" x="0.30" y="0.40" z="0.02">
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_box>
</link>
<gazebo reference="tray">
    <material>Gazebo/DarkGrey</material>
</gazebo>

<joint name="pole_to_tray" type="fixed">
  <parent link="tray_pole"/>
  <child link="tray"/>
  <origin xyz="0.2 0 0" rpy="0 0 0"/>
</joint>

</robot>
